/*----------------------------------------------------------------------------*/
/* QLineEdit                */
/*----------------------------------------------------------------------------*/
QLineEdit {
        background: rgb(255, 255, 255);
	color: rgb(  0,   0,   0);
        font-size: 11px;
        min-height: 1.6em;
        border: solid 0px;
        padding: 0 3 0 3 px;
}

QLineEdit:disabled {
        background: rgb(255, 255, 255);
	color: rgb(141, 141, 141);
}

QLineEdit:read-only {
        background: rgb(230, 230, 230);
}

/*----------------------------------------------------------------------------*/
/* QScrollBar (vertical) */
/*----------------------------------------------------------------------------*/
QScrollBar:vertical {
        border: 0px;
        background: rgb( 46,  46,  46);
        width: 14px;
        padding-top:14px;
        padding-bottom:14px;
        padding-left:2px;
        padding-right:2px;
        max-width:10px;
        border-radius: 0px;
}

QScrollBar::handle:vertical {
        background: rgb( 83,  83,  83);
        min-height: 16px;
        width: 8px;
        border-radius: 5px;
        border: 0px solid grey;
}

QScrollBar::handle:vertical:hover {
        background: rgb(114, 114, 114);
}

QScrollBar::handle:vertical:disabled {
        background: rgb( 58,  58,  58);
}

QScrollBar::sub-line:vertical {
        height: 14px;
        border:0px;
        image:url(theme:scrollbar_up.png);
        subcontrol-position: top;
        subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical:pressed {
        image:url(theme:scrollbar_up_down.png);
}

QScrollBar::sub-line:vertical:disabled {
        image:url(theme:scrollbar_up_disable.png);
}

QScrollBar::add-line:vertical {
        height: 14px;
        border:0px;
        image:url(theme:scrollbar_down.png);
        subcontrol-position: bottom;
        subcontrol-origin: margin;
}

QScrollBar::add-line:vertical:pressed {
        image:url(theme:scrollbar_down_down.png);
}

QScrollBar::add-line:vertical:disabled {
        image:url(theme:scrollbar_down_disabled.png);
}

QScrollBar::left-arrow:vertical {
        border: 0px solid grey;
        height: 0px;
        width: 0px;
}

QScrollBar::right-arrow:vertical {
        border: 0px solid grey;
        height: 0px;
        width: 0px;
}

QScrollBar::add-page:vertical {
        background: none;
        border: 0px;
}

QScrollBar::sub-page:vertical {
        background: none;
        border: 0px;
}

/*----------------------------------------------------------------------------*/
/* QScrollBar (horizontal) */
/*----------------------------------------------------------------------------*/
QScrollBar:horizontal {
        border: 0px;
        background: rgb( 46,  46,  46);
        height: 14px;
        padding-top:2px;
        padding-bottom:2px;
        padding-left:14px;
        padding-right:14px;
        max-height:10px;
        border-radius: 0px;
}

QScrollBar::handle:horizontal {
        background: rgb( 83,  83,  83);
        min-width: 16px;
        height: 8px;
        border-radius: 5px;
        border: 0px solid grey;
}

QScrollBar::handle:horizontal:hover {
        background: rgb(114, 114, 114);
        min-width: 16px;
        height: 8px;
        border-radius: 5px;
        border: 0px solid grey;
}

QScrollBar::handle:horizontal:disabled {
        background: rgb( 58,  58,  58);
        min-width: 16px;
        height: 8px;
        border-radius: 5px;
        border: 0px solid grey;
}

QScrollBar::add-line:horizontal {
        width: 14px;
        border:0px;
        image:url(theme:scrollbar_right.png);
        subcontrol-position: right;
        subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal:pressed {
        image:url(theme:scrollbar_right_down.png);
}

QScrollBar::add-line:horizontal:disabled {
        image:url(theme:scrollbar_right_disable.png);
}

QScrollBar::sub-line:horizontal {
        width: 14px;
        border:0px;
        image:url(theme:scrollbar_left.png);
        subcontrol-position: left;
        subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal:pressed {
    image:url(theme:scrollbar_left_down.png);
}

QScrollBar::sub-line:horizontal:disabled {
        image:url(theme:scrollbar_left_disable.png);
}

QScrollBar::up-arrow:horizontal {
        border: 0px solid grey;
        height: 0px;
        width: 0px;
}

QScrollBar::down-arrow:horizontal {
        border: 0px solid grey;
        height: 0px;
        width: 0px;
}

QScrollBar::add-page:horizontal {
        background: none;
        border: 0px;
}

QScrollBar::sub-page:horizontal {
        background: none;
        border: 0px;
}

/*----------------------------------------------------------------------------*/
/*QGroupBox*/
/*----------------------------------------------------------------------------*/

QGroupBox {
        border: 1px solid rgb(202, 202, 202);
        border-radius: 0px;
        margin-top: 2ex;
        padding-bottom:0px;
        background: transparent;
        font-size: 11px;
}

QGroupBox:indicator {
        width: 13px;
        height: 12px;
}

QGroupBox::indicator:unchecked {
        image: url(theme:checkbox_off.png);
}

QGroupBox::indicator:unchecked:pressed {
        image: url(theme:checkbox_off_down.png);
}

QGroupBox::indicator:indeterminate {
        image: url(theme:checkbox_off.png);
}

QGroupBox::indicator:indeterminate:pressed {
        image: url(theme:checkbox_off_down.png);
}

QGroupBox::indicator:checked {
        image: url(theme:checkbox_on.png);
}

QGroupBox::indicator:checked:pressed {
        image: url(theme:checkbox_on_down.png);
}

QGroupBox::indicator:checked:disabled {
        image: url(theme:checkbox_on_disable.png);
}

QGroupBox::indicator:unchecked:disabled {
        image: url(theme:checkbox_off_disable.png)
}

QGroupBox::title {
        color: rgb(255, 255, 255);
        padding: 0 3px;
        subcontrol-origin:margin;
