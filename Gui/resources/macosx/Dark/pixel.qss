/*----------------------------------------------------------------------------*/
/* QWidget*/
/*----------------------------------------------------------------------------*/
QWidget	{
	background: rgb(102, 102, 102);
}

QWidget#m_pHorScaleWidget {
        background: transparent;
}

QWidget#m_pVerScaleWidget {
        background: transparent;
}

QWidget#OPCOMBOBOXPOPUPWIDGETITEMAREA {
        background: transparent;
        color: rgb(202, 202, 202);
}

QWidget#ChannelDelegate
{
        background: transparent;
}

/*----------------------------------------------------------------------------*/
/* OPMainWindow*/
/*----------------------------------------------------------------------------*/

OPMainWindow QWidget#OPMAINWINDOWCONTENT {
        background: rgb( 51,  51,  51);
        border: none;
        border-radius: 0px;
}

QMainWindow {
        border: none;
        border-radius: 0px;
        icon-size: 20px;
        background: transparent;
}

/*----------------------------------------------------------------------------*/
/* OPMdiAreaWidget*/
/*----------------------------------------------------------------------------*/
OPMdiAreaWidget {
        background: transparent;
        /*border: 1px solid rgb(202, 202, 202);*/
        border-radius: 0px;
        padding: 0px;
}

/*----------------------------------------------------------------------------*/
/* OPWidget & OPDialog*/
/*----------------------------------------------------------------------------*/
OPWidget .QWidget#OPWIDGETCONTENT, OPDialog .QWidget#OPDIALOGCONTENT {
        background: rgb(102, 102, 102);
        border: none;
}

OPWidget .QWidget#OPWIDGETMAINCONTENT
{
        border-left: 3px solid rgb( 76,  76,  76);
        border-right: 3px solid rgb( 76,  76,  76);
        border-bottom: 3px solid rgb( 76,  76,  76);
        border-bottom-left-radius: 3px;
        border-bottom-right-radius: 3px;
}
/*----------------------------------------------------------------------------*/
/* DocumentWidget*/
/*----------------------------------------------------------------------------*/
DocumentWidget {
        background: transparent;
}

/*----------------------------------------------------------------------------*/
/* OPDocumentView*/
/*----------------------------------------------------------------------------*/
OPDocumentView {
        background: rgb( 36,  36,  36);
}

/*============================================================================*/
/*============================================================================*/
/*============================================================================*/
/* Main controls       */
/*============================================================================*/
/*============================================================================*/
/*============================================================================*/

/*----------------------------------------------------------------------------*/
/* QMenuBar*/
/*----------------------------------------------------------------------------*/
QMenuBar {
        background-color: rgb( 41,  41,  41);
        font-size: 13px;
}

QMenuBar::item {
        background-color: rgb( 41,  41,  41);
        padding: 4px 9px;
        color: rgb(162, 162, 162);
}

QMenuBar::item:disabled {
        background-color: rgb( 41,  41,  41);
        color: rgb( 92,  92,  92);
        font-size: 13px;
        padding: 4px 9px;
}

QMenuBar::item:selected {
        background: rgb( 78, 132, 196);
        color: rgb(255, 255, 255);
        font-size: 13px;
 }

QMenuBar::item:pressed {
        background: rgb( 78, 132, 196);
        color: rgb(255, 255, 255);
        font-size: 13px;
}

/*----------------------------------------------------------------------------*/
/* QMenu*/
/*----------------------------------------------------------------------------*/
QMenu {
        background-color: rgb( 41,  41,  41);
}

QMenu::separator {
        height: 1px;
        margin: 3px 5px;
        background-color: rgb(162, 162, 162);
}

QMenu::item {
        background-color: rgb( 41,  41,  41);
        color: rgb(162, 162, 162);
        font-size: 13px;
        padding: 4px 20px 4px 20px;
}

QMenu::item:disabled {
        background-color: rgb( 41,  41,  41);
        color: rgb( 92,  92,  92);
        font-size: 13px;
        padding: 4px 20px 4px 20px;
}

QMenu::item:disabled:selected {
        background-color: rgb( 41,  41,  41);
        color: rgb( 92,  92,  92);
        font-size: 13px;
        padding: 4px 20px 4px 20px;
}

QMenu::item:selected {
        background: rgb( 78, 132, 196);
        color: rgb(255, 255, 255);
        font-size: 13px;
}

QMenu::item:pressed {
        background: rgb( 78, 132, 196);
        color: rgb(255, 255, 255);
        font-size: 13px;
        padding: 5px 19px 3px 21px;
}

QMenu::indicator {
        width: 13px;
        height: 13px;
}

/*----------------------------------------------------------------------------*/
/* QLabel                         */
/*----------------------------------------------------------------------------*/
QLabel {
        color: rgb(202, 202, 202);
        background: transparent;
        font-size: 11px;
}

QLabel :disabled {
        color: rgb(141, 141, 141);
        background: transparent;
}

/*----------------------------------------------------------------------------*/
/* QPushButton                  */
/*----------------------------------------------------------------------------*/

QPushButton {
        font-size: 11px;
        border: none;
        min-height: 1.8em;
	color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 56,  56,  56),
				stop: 1 rgb( 41,  41,  41));
 }

QPushButton:hover {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 76,  76,  76),
				stop: 1 rgb( 59,  59,  59));
 }

QPushButton:pressed {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 41,  41,  41),
				stop: 1 rgb( 56,  56,  56));
 }

QPushButton:checked {
        border: 3px solid rgb( 78, 132, 196);
        color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 56,  56,  56),
				stop: 1 rgb( 41,  41,  41));
}

QPushButton:hover:checked {
        border: 3px solid rgb( 78, 132, 196);
        color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 56,  56,  56),
				stop: 1 rgb( 41,  41,  41));
}

QPushButton:disabled {
        color: rgb( 91,  91,  91);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 71,  71,  71),
				stop: 1 rgb( 51,  51,  51));
 }

QPushButton:default {
        color: rgb(238, 238, 238);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb(114, 162, 209),
				stop: 1 rgb( 90, 141, 196));
 }

QPushButton:hover:default {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb(123, 169, 213),
				stop: 1 rgb( 81, 132, 190));
 }

QPushButton:pressed:default {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 82, 133, 191),
				stop: 1 rgb(120, 167, 211));
 }

 QPushButton:disabled:default {
        color: rgb( 91,  91,  91);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 71,  71,  71),
				stop: 1 rgb( 51,  51,  51));
 }

/*----------------------------------------------------------------------------*/
/*QToolButton               */
/*----------------------------------------------------------------------------*/
QToolButton {
	border: none;
	font-size: 11px;
 	color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 61,  61,  61),
				stop: 1 rgb( 41,  41,  41));
}

QToolButton:hover {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 79,  79,  79),
				stop: 1 rgb( 57,  57,  57));
}

QToolButton:pressed {
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 39,  39,  39),
				stop: 1 rgb( 58,  58,  58));
}

QToolButton:disabled {
        color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 61,  61,  61),
				stop: 1 rgb( 41,  41,  41));
}

QToolButton:checked {
        font-size: 11px;
	border: 3px solid rgb( 78, 132, 196);
        color: rgb(202, 202, 202);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 61,  61,  61),
				stop: 1 rgb( 41,  41,  41));
}

QToolButton:hover:checked {
        font-size: 11px;
        border: 3px solid rgb( 78, 132, 196);
        color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 79,  79,  79),
				stop: 1 rgb( 57,  57,  57));
}

/*----------------------------------------------------------------------------*/
/* QAbstractItemView*/
/*----------------------------------------------------------------------------*/
QAbstractItemView {
	margin: 0px;
	padding: 0px;
	font-size: 11px;
        icon-size: 32px;
        background: transparent;
        border-radius: 0px;
}

QAbstractItemView::item {
	margin: 0px;
        padding: 3px 14px 3px;
        color: rgb(202, 202, 202);
        border-bottom: 1px solid rgb( 72,  72,  72);
}

QAbstractItemView::item:last {
        padding: 3px 14px 3px;
        border: none;
}

QAbstractItemView::item:selected {
        color: rgb(202, 202, 202);
        border: none;
        border-radius: 0px;
        border-bottom: 1px solid rgb( 72,  72,  72);
        background: rgb(115, 115, 115);
}

QAbstractItemView::item:hover {
        color: rgb(202, 202, 202);
        border: none;
        border-radius: 0px;
        border-bottom: 1px solid rgb( 72,  72,  72);
        background: rgb(115, 115, 115);
}

QAbstractItemView::indicator {
        background: transparent;
}

QAbstractItemView::indicator:checked {
        image: url(theme:checkbox_on.png);
}

QAbstractItemView::indicator:unchecked {
        image: url(theme:checkbox_off.png);
}

/*----------------------------------------------------------------------------*/
/* QListView*/
/*----------------------------------------------------------------------------*/
QListView {
        background: transparent;
        border: 0px;
        font-size: 11px;
}

/*----------------------------------------------------------------------------*/
/* QComboBox                    */
/*----------------------------------------------------------------------------*/
QComboBox {
        border: 0px;
        border-style: solid;
	min-height: 1.8em;
        font-size: 11px;
        padding: 0px 4px 0px 4px;
	color: rgb(255, 255, 255);
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 56,  56,  56),
                                stop: 1 rgb( 41,  41,  41));
}

QComboBox:on {
        color: rgb(255, 255, 255);
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 41,  41,  41),
                                stop: 1 rgb( 56,  56,  56));
}

QComboBox:hover {
        color: rgb(255, 255, 255);
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 76,  76,  76),
                                stop: 1 rgb( 59,  59,  59));
}

QComboBox:disabled {
        color: rgb( 91,  91,  91);
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                stop: 0 rgb( 71,  71,  71),
                                stop: 1 rgb( 51,  51,  51));
}

QComboBox::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 10px;
        border:0px solid;
        border-radius:0px;
}

QComboBox::down-arrow {
        image:url(theme:combo_arrow_small.png);
}

QComboBox::down-arrow:disabled {
        image:url(theme:combo_arrow_small_disable.png);
}

QComboBox::down-arrow:on {
        image:url(theme:combo_arrow_small_up.png);
}

/*----------------------------------------------------------------------------*/
/*QCheckBox             */
/*----------------------------------------------------------------------------*/

QCheckBox {
	font-size: 11px;
        color: rgb(202, 202, 202);
        background: transparent;
}

QCheckBox:hover {
	font-size: 11px;
        color: rgb(255, 255, 255);
        background: transparent;
}

QCheckBox:pressed {
	font-size: 11px;
        color: rgb(137, 195, 245);
        background: transparent;
}

QCheckBox:disabled {
        color: rgb(141, 141, 141);
        background: transparent;
}

QCheckBox::indicator {
        width: 13px;
        height: 12px;
        padding-right:3px;
}

QCheckBox::indicator:unchecked {
        image: url(theme:checkbox_off.png);
}

QCheckBox::indicator:indeterminate {
        image: url(theme:checkbox_off.png);
}

QCheckBox::indicator:unchecked:pressed {
        image: url(theme:checkbox_off_down.png);
}

QCheckBox::indicator:indeterminate:pressed {
        image: url(theme:checkbox_off_down.png);
}

QCheckBox::indicator:checked {
        image: url(theme:checkbox_on.png);
}

QCheckBox::indicator:checked:pressed {
        image: url(theme:checkbox_on_down.png);
}

QCheckBox::indicator:checked:disabled {
        image: url(theme:checkbox_on_disable.png);
}

QCheckBox::indicator:unchecked:disabled {
        image: url(theme:checkbox_off_disable.png)
}

/*----------------------------------------------------------------------------*/
/* QRadioButton */
/*----------------------------------------------------------------------------*/
QRadioButton {
        font-size: 11px;
        color: rgb(202, 202, 202);
        background: transparent;
}

QRadioButton:hover {
        font-size: 11px;
        color: rgb(255, 255, 255);
        background: transparent;
}

QRadioButton:pressed {
        font-size: 11px;
        color: rgb(137, 195, 245);
        background: transparent;
}

QRadioButton:disabled {
        color: rgb(141, 141, 141);
        background: transparent;
}

QRadioButton::indicator {
        width: 12px;
        height: 12px;
        padding-right:3px;
}

QRadioButton::indicator:unchecked {
        image: url(theme:radio_off.png);
}

QRadioButton::indicator:unchecked:pressed {
        image: url(theme:radio_off_down.png);
}

QRadioButton::indicator:checked {
        image: url(theme:radio_on.png);
}

QRadioButton::indicator:checked:pressed {
        image: url(theme:radio_on_down.png);
}

QRadioButton::indicator:checked:disabled {
