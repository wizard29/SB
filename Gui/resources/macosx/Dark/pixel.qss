/*----------------------------------------------------------------------------*/
/* QSpinBox                     */
/*----------------------------------------------------------------------------*/

QSpinBox {
        background: rgb(255, 255, 255);
	color: rgb(  0,   0,   0);
        border: solid 0px;
        padding-left: 3px;
        padding-right: 1px;
        font-size: 11px;
        min-height: 1.6em;
}

QSpinBox:disabled {
        background: rgb(255, 255, 255);
	color: rgb(141, 141, 141);
}

QSpinBox:read-only {
        background: rgb(230, 230, 230);
}

QSpinBox::up-button {
        subcontrol-origin: border;
        subcontrol-position: top right; /* position at the top right corner */
        width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
        border-width: 0px;
}

QSpinBox::up-arrow {
        image: url(theme:spinbox_up.png);
        width: 16px;
        height: 11px;
}

QSpinBox::up-arrow:pressed {
        image: url(theme:spinbox_up_hover.png);
}

QSpinBox::up-arrow:disabled {
        image: url(theme:spinbox_up_disable.png);
}

QSpinBox::up-arrow:off {
        image: url(theme:spinbox_up_disable.png);
}

QSpinBox::down-button {
        subcontrol-origin: border;
        subcontrol-position: bottom right; /* position at bottom right corner */
        width: 16px;
        border-width: 0px;
        border-top-width: 0;
}

QSpinBox::down-arrow {
        image: url(theme:spinbox_down.png);
        width: 16px;
        height: 11px;
}

QSpinBox::down-arrow:pressed {
        image: url(theme:spinbox_down_hover.png);
}

QSpinBox::down-arrow:disabled {
        image: url(theme:spinbox_down_disable.png);
}

QSpinBox::down-arrow:off {
        image: url(theme:spinbox_down_disable.png);
}

